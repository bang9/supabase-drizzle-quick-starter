{
  "extends": "@starter/tsconfig",
  "compilerOptions": {
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "outDir": "./dist",
    "baseUrl": "./",
    "paths": {
      "$*": ["src/*"]
    }
  },
  "ts-node": {
    "esm": true,
    "preferTsExts": false,
    "experimentalSpecifierResolution": "node"
  }
}
